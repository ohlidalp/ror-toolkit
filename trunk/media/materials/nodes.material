
material TruckEditor/NodeNormal
{
    technique
    {
        pass
        {
			lighting off
			depth_check on
            cull_hardware none 
            scene_blend alpha_blend
            depth_write off
            texture_unit
            {
                colour_op_ex source1 src_manual src_current 0 0 0.5
                alpha_op_ex source1 src_manual src_current 1
            }
        } 
    }
}

